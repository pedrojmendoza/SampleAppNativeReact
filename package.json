{
  "name": "my-native-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "jest": "^22.4.3",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "jest-react-native": "^18.0.0",
    "react-test-renderer": "^16.3.0-alpha.1",
    "appium": "^1.6.5",
    "appium-doctor": "^1.4.3",
    "wd": "^1.4.0"
  },
  "scripts": {
    "debug:android:es": "ENVFILE=.env.es react-native run-android",
    "debug:android:us": "ENVFILE=.env.us react-native run-android",
    "release:android:es": "cd android && ENVFILE=.env.es ./gradlew assembleRelease && cd ..",
    "release:android:us": "cd android && ENVFILE=.env.us ./gradlew assembleRelease && cd ..",
    "ios": "react-native run-ios",
    "start:appium": "appium",
    "test": "jest --testMatch=\"**/tests/**/!(*e2e.js)\"",
    "test:e2e:android": "source venv/bin/activate && LOCATION='local' py.test && deactivate",
    "beta:android:es": "cd ./android && fastlane beta_es",
    "beta:android:us": "cd ./android && fastlane beta_us",
    "promote:android:es": "cd ./android && fastlane promote_es",
    "promote:android:us": "cd ./android && fastlane promote_us"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|tcomb-form-native|react-native-config)/)"
    ],
    "modulePathIgnorePatterns": [
      "npm-cache",
      ".npm"
    ]
  },
  "dependencies": {
    "react": "16.3.1",
    "react-native": "0.55.1",
    "react-native-config": "^0.11.5",
    "tcomb-form-native": "^0.6.11"
  }
}
